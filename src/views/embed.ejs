    <div id="tip-modal" class="bubble-modal">
        <h3>Send a Tip</h3>
        <ul class="tip-amounts">
            <li>
                <input type="radio" name="tip-amount" id="tip-option-0" value="0.25" onclick="changeTipAmount(this);" />
                <label for="tip-option-0">25&cent;</label>
            </li>
            <li>
                <input type="radio" name="tip-amount" id="tip-option-1" value="0.50" onclick="changeTipAmount(this);" />
                <label for="tip-option-1">50&cent;</label>
            </li>
            <li>
